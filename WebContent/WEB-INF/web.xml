    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <resource-ref>
    <description></description>
    <res-ref-name>jms/cf1.1Ref</res-ref-name>
    <res-type>javax.jms.ConnectionFactory</res-type>
    <res-sharing-scope>Shareable</res-sharing-scope>
    <mapped-name>jms/cf1.1</mapped-name>
  </resource-ref>
  <resource-ref>
    <description></description>
    <res-ref-name>jms/queue1.1Ref</res-ref-name>
    <res-type>javax.jms.Queue</res-type>
    <res-sharing-scope>Shareable</res-sharing-scope>
    <mapped-name>jms/queue1.1</mapped-name>
  </resource-ref>
  
  
      <servlet>
        <description/>
        <display-name>AsynchReadingServlet</display-name>
        <servlet-name>AsynchReadingServlet</servlet-name>
        <servlet-class>jhc.listener.AsynchReadingServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>AsynchReadingServlet</servlet-name>
        <url-pattern>/AsynchReadingServlet</url-pattern>
    </servlet-mapping>
    
    
</web-app>